#include <iostream>
using namespace std;
int main()
{
    // to print  hollow rectangle with star pattern
    int rows = 5;    // Defining the size of the rows
    int column = 10; // Defining the size of the column

    for (int i = 0; i < rows; ++i)
    {
        for (int j = 0; j < column; ++j)
        {
            if ((i == 0 || j == column - 1) || (j == 0 || i == rows - 1))
            {
                cout << "* ";
            }

            else
            {
                cout << "  ";
            }
        }

        cout << endl;
    }

    return 0;
}
